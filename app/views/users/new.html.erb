<%= form_for @user do |f| %>
  <p>
    <%= f.label :FirstName %><br>
    <%= f.text_field :f_name %>
  </p>
 

 
 <p>
    <%= f.label :LastName%><br>
    <%= f.text_field :l_name %>
  </p>

  <p>
    <%= f.label :UserName %><br>
    <%= f.text_field :u_name %>
  </p>

  

  <p>
    <%= f.label :Email %><br>
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.label :Password %><br>
    <%= f.text_field :password %>
  </p>


  <p>
    <%= f.label :Image %><br>
    <%= f.file_field :image %>
  </p>
  <p>

    <%= f.submit %>
  </p>
<% end %>



